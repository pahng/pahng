{
  "name": "pahng",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && npm run serve",
    "build": "npm run build:angular && npm run build:server",
    "build:angular": "ng build --prod",
    "build:server": "tsc -p tsconfig.server.json",
    "watch:server": "tsc -w -p tsconfig.server.json",
    "watch:angular": "ng serve --proxy-config proxy.config.json",
    "watch": "npm run build:server && concurrently -k -p \"[{name}]\" -n \"Angular,TypeScript,Node\" -c \"cyan.bold,green.bold,red.bold\" \"npm run watch:angular\" \"npm run watch:server\" \"nodemon dist/server.js\"",
    "serve": "node dist/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": "8.9.3",
    "npm": "5.5.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "body-parser": "~1.18.2",
    "core-js": "^2.4.1",
    "express": "^4.11.2",
    "pg": "^7.4.1",
    "rxjs": "^5.5.6",
    "semantic-ui-css": "^2.2.12",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/language-service": "^5.1.3",
    "@angular/service-worker": "^5.1.3",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.96",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.14.7",
    "protractor": "~5.1.2",
    "ts-node": "^3.2.2",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2"
  }
}
